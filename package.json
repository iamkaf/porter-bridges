{
  "name": "porter-bridges",
  "version": "1.0.0",
  "description": "A development utility for discovering, processing, and packaging Minecraft mod porting data.",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "bun src/index.ts orchestrate",
    "orchestrate": "bun src/index.ts orchestrate",
    "run": "bun src/index.ts run",
    "discover": "bun src/index.ts discover",
    "collect": "bun src/index.ts collect",
    "distill": "bun src/index.ts distill",
    "package": "bun src/index.ts package",
    "bundle": "bun src/index.ts bundle",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf generated/*",
    "clean:full": "rm -rf generated/ && mkdir generated",
    "lint": "ultracite lint",
    "format": "ultracite format",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "minecraft",
    "modding",
    "porting",
    "fabric",
    "forge",
    "neoforge"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "archiver": "^7.0.1",
    "commander": "^11.0.0",
    "ky": "^1.8.1",
    "listr2": "^8.0.1",
    "ora": "^8.0.1",
    "remeda": "^2.25.0",
    "undici": "^7.11.0",
    "winston": "^3.17.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.1",
    "@types/archiver": "^6.0.3",
    "@types/node": "^24.0.13",
    "typescript": "^5.8.3",
    "ultracite": "^5.0.36"
  }
}
