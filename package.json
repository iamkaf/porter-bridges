{
  "name": "porter-bridges",
  "version": "1.0.0",
  "description": "A development utility for discovering, processing, and packaging Minecraft mod porting data.",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "bun src/index.ts orchestrate",
    "orchestrate": "bun src/index.ts orchestrate",
    "run": "bun src/index.ts run",
    "discover": "bun src/index.ts discover",
    "collect": "bun src/index.ts collect",
    "distill": "bun src/index.ts distill",
    "package": "bun src/index.ts package",
    "bundle": "bun src/index.ts bundle",
    "typecheck": "tsc --noEmit",
    "clean": "find generated -mindepth 1 -maxdepth 1 ! -name 'distilled-content' -exec rm -rf {} +",
    "clean:full": "rm -rf generated/ && mkdir generated",
    "lint": "ultracite lint",
    "format": "ultracite format",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "minecraft",
    "modding",
    "porting",
    "fabric",
    "forge",
    "neoforge"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/rest": "^2.5.1",
    "@types/figlet": "^1.7.0",
    "@types/inquirer": "^9.0.8",
    "archiver": "^7.0.1",
    "boxen": "^8.0.1",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "commander": "^11.0.0",
    "commander-completion": "^1.0.1",
    "compromise": "^14.14.4",
    "dirty-json": "^0.9.2",
    "discord-api-types": "^0.38.16",
    "discord.js": "^14.21.0",
    "figlet": "^1.8.2",
    "gradient-string": "^3.0.0",
    "inquirer": "^12.7.0",
    "jsonc-parser": "^3.3.1",
    "jsonrepair": "^3.13.0",
    "ky": "^1.8.1",
    "listr2": "^8.0.1",
    "lru-cache": "^11.0.1",
    "ml-distance": "^4.0.1",
    "natural": "^8.1.0",
    "ora": "^8.2.0",
    "p-limit": "^6.1.0",
    "pako": "^2.1.0",
    "remeda": "^2.25.0",
    "simple-git": "^3.28.0",
    "stream-json": "^1.9.1",
    "strip-json-comments": "^5.0.2",
    "undici": "^7.11.0",
    "update-notifier": "^7.3.1",
    "winston": "^3.17.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.1",
    "@types/archiver": "^6.0.3",
    "@types/node": "^24.0.13",
    "@types/pako": "^2.0.3",
    "typescript": "^5.8.3",
    "ultracite": "^5.0.36"
  }
}
